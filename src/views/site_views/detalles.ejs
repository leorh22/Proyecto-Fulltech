                    <tr><th>TIPO DE DISPOSITIVO</th><td><%= data.tipo_dispositivo %></td></tr>
                    <tr><th>MARCA</th><td><%= data.marca %></td></tr>
                    <tr><th>MODELO</th><td><%= data.modelo %></td></tr>
                    <tr><th>NÃšMERO DE SERIE</th><td><%= data.no_serie %></td></tr>
                    <tr><th>ESTADO DEL EQUIPO</th><td><%= data.estado_dispositivo %></td></tr>
                    <tr><th>COMENTARIO</th><td><%= data.comentario %></td></tr>
                    <% } %>
                </table>
            </div>
        </div>
    </div>
</body>
</html>